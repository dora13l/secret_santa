<div class="container">
  <h1>All events</h1>

  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Location</th>
        <th>Price</th>
        <th>User</th>
        <th colspan="3"></th>
      </tr>
    <thead>

    <tbody>
      <% @events.each do |event| %>
        <tr>
          <td><%= event.name %></td>
          <td><%= event.location%></td>
          <td><%= event.price%></td>
          <td><%= event.user_id%></td>
        </tr>
      <% end %>
    </tbody>
  <table>

</div>
